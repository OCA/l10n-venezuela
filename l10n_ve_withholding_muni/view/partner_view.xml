<?xml version='1.0' encoding='UTF-8'?>
<openerp>
    <data>



    <!--
    =====================================================
    Partners Extension
    =====================================================
    -->

        <record id="partner_ext_munici_view_form_loc_ve" model="ir.ui.view">
            <field name="name">partner_extended_munici.partner.form.view.loc_ve</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="l10n_ve_withholding.wh_partner"/>
            <field name="arch" type="xml">
                <page string="Withholdings" position="inside">
                    <notebook>
                        <page string="Local Withholdings">
                            <group col="4" colspan="2">
                                <field name="property_wh_munici_payable" attrs="{'required':'True'}"/>
                                <field name="property_wh_munici_receivable" attrs="{'required':'True'}"/>
                            </group>
                        </page>
                    </notebook>
                </page>
            </field>
        </record>

    </data>
</openerp>
