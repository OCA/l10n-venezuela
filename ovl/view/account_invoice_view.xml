<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="account_invoice_journal_entries_form">
            <field name="name">account.invoice.journal.entries.form</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="l10n_ve_withholding.wh_inv_supplier"/>
            <field name="arch" type="xml">
                <xpath expr="//separator[@string='xsep']" position="before">
                    <button string="Journal Entries" name="show_entries" type="object" class="oe_highlight" states="open,paid"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
